{
    "env": {
        "browser": true,
        "commonjs": true,
		"es2021": true,
		"jquery": true
    },
    "extends": [
		"eslint:recommended",
		"plugin:node/recommended",
		"prettier"
	],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier":"error",
		"no-unused-vars":"warn",
		"func-names":"off",
		"no-underscore-dangle": ["error", { "allow": ["_id"] }]
	},
	"globals": {
    	"logger": true
	}
}